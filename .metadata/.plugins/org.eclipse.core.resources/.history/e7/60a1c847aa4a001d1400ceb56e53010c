package common;
import java.util.Random;

public class WeiCalc {
	public int wei;
	public int i;
	public int j;
	public int k;
	
	
	public WeiCalc() {}
	public WeiCalc(int a) {
		this.wei = a;
	}
	
	public int calc( ) {
		for(i=0; i<=wei;i++) {
			for(j=0;j<=wei;j++) {
				for(k=0;k<=wei;k++) {
					k = (int)(Math.random()*10)*5;
					if (k < wei) {
						wei += k;
						this.k = k;
					}else break;
					
				}
			}
		}
	}
	
	public void disp() {
		
	}
	
}
