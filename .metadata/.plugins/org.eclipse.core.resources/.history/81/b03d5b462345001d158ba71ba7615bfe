<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%  int num = 0;
	int f = 1;
	String snum = request.getParameter("num");
	if(snum!=null && snum.equals("")==false) {
		num = Integer.parseInt(snum);
	}
	for (int i = 1; i <= num;i++) {
		int f = (i-1)*i;
	}
	%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<h2><%= f %></h2>
</body>
</html>