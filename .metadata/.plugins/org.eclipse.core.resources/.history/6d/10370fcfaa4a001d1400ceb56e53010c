package common;
import java.util.Random;

public class WeiCalc {
	public int wei;
	public int i;
	public int j;
	public int k;
	int k = 20;
	
	public WeiCalc() {}
	public WeiCalc(int a) {
		this.wei = a;
	}
	
	public void calc( ) {
		for(i=0; i<=wei;i++) {
			for(j=0;j<=wei;j++) {
				for(k=0;k<=wei;k++) {
					k = (int)(Math.random()*10)*5;
					wei += k;
					this.k = k;
				}
			}
		}
	}
	
	public int getk() {
		return k;
	}
	
}
